{
  "version": 2,
  "builds": [
    {
      "src": "client/**",
      "use": "@vercel/node"
    },
    {
      "src": "api/**",
      "use": "vercel-python@0.0.3"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}
